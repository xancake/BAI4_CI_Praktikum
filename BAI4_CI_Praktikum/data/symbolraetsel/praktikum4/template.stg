group template;

sums(symbols,sums) ::= <<
	<sums; separator="\n\n">
>>

sum(number1, number2, number3) ::= <<
	<number1.characters, number2.characters, number3.characters :
	{n1, n2, n3 | <if(n1)><n1><else>0<endif> + <if(n2)><n2><else>0<endif> = <if(n3)><n3><else>0<endif>;}
	; separator="\n">
>>
